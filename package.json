{
  "name": "MPC-DiscordRPC-Thumbnail",
  "version": "0.5.1",
  "description": "Discord Rich Presence for Media Player Classic with enhanced thumbnail support",
  "main": "main.js",
  "repository": "https://github.com/aqemi/MPC-DiscordRPC-Electron.git",
  "bugs": "https://github.com/aqemi/MPC-DiscordRPC-Electron/issues",
  "author": "angeloanan <anandavid2002@gmail.com>",
  "contributors": [
    "Der-Eddy (https://www.eddy-dev.net)",
    "Lucas Miranda <liddack@outlook.com>",
    "MaciejGorczyca (https://github.com/MaciejGorczyca)",
    "aqemi (https://github.com/aqemi)"
  ],
  "os": [
    "win32"
  ],
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "build": "electron-builder build",
    "pack": "electron-builder build --dir",
    "dev:builder": "electron-builder start",
    "dev": "nodemon --signal SIGINT --exec electron .",
    "dev:node": "nodemon --signal SIGINT",
    "start": "del /F /Q mpc-discordrpc.log && pm2 start pm2.config.js",
    "stop": "pm2 delete mpc-discordrpc"
  },
  "license": "MIT",
  "dependencies": {
    "@xhayper/discord-rpc": "^1.0.22",
    "axios": "^0.24.0",
    "discord-rpc": "^4.0.1",
    "fancy-log": "^1.3.2",
    "form-data": "^4.0.2",
    "imgur": "^2.4.3",
    "jsdom": "^11.6.2",
    "pm2": "^5.1.2",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "electron": "^26.2.4",
    "nodemon": "^3.0.1"
  }
}
